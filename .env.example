# Copy to .env and fill real values (do not commit .env)

# Required for selected provider
GROQ_API_KEY=
MISTRAL_API_KEY=
OPENAI_API_KEY=

# Langfuse (optional)
LANGFUSE_SECRET_KEY=
LANGFUSE_PUBLIC_KEY=
LANGFUSE_BASE_URL=https://cloud.langfuse.com

# LangSmith (optional)
# LANGSMITH_API_KEY=
# LANGSMITH_TRACING=true
# LANGSMITH_ENDPOINT=https://eu.api.smith.langchain.com
# LANGSMITH_PROJECT=CyberneticAgents

# LLM config (optional)
LLM_PROVIDER=groq
GROQ_MODEL=llama-3.3-70b-versatile
MISTRAL_MODEL=mistral-small-latest
OPENAI_MODEL=gpt-5-nano-2025-08-07
LLM_MAX_TOKENS=4096
GROQ_BASE_URL=https://api.groq.com/openai/v1
OPENAI_BASE_URL=https://api.openai.com/v1

# Telegram (optional)
TELEGRAM_BOT_TOKEN=
TELEGRAM_BOT_USERNAME=
TELEGRAM_WEBHOOK_URL=
TELEGRAM_WEBHOOK_SECRET=
TELEGRAM_WEBHOOK_HOST=0.0.0.0
TELEGRAM_WEBHOOK_PORT=8080
TELEGRAM_ALLOWLIST_CHAT_IDS=
TELEGRAM_ALLOWLIST_USER_IDS=
TELEGRAM_BLOCKLIST_CHAT_IDS=
TELEGRAM_BLOCKLIST_USER_IDS=
TELEGRAM_PAIRING_ENABLED=1
TELEGRAM_PAIRING_ADMIN_CHAT_IDS=

# Runtime queue backend (optional)
CYBERAGENT_RUNTIME_QUEUE_BACKEND=file
CYBERAGENT_RUNTIME_QUEUE_DB_PATH=data/runtime_queue.db

# Memory (optional)
MEMORY_BACKEND=chromadb
MEMORY_SQLITE_PATH=data/memory.db
MEMORY_CHROMA_COLLECTION=memory_store
MEMORY_CHROMA_PATH=data/chroma_db
MEMORY_CHROMA_HOST=
MEMORY_CHROMA_PORT=8000
MEMORY_CHROMA_SSL=false
MEMORY_VECTOR_BACKEND=none
MEMORY_VECTOR_COLLECTION=memory_vectors
MEMORY_INJECTION_MAX_CHARS=1200
MEMORY_INJECTION_PER_ENTRY_MAX_CHARS=400
MEMORY_RETRIEVAL_LIMIT=6
MEMORY_TEAM_NAMESPACE=team_1
MEMORY_GLOBAL_NAMESPACE=user
MEMORY_METRICS_LOG=false
MEMORY_METRICS_LOG_INTERVAL_SECONDS=60
MEMORY_SESSION_LOGGING=true
MEMORY_SESSION_LOG_MAX_CHARS=2000
MEMORY_COMPACTION_THRESHOLD_CHARS=8000
MEMORY_REFLECTION_INTERVAL_SECONDS=3600

# Taiga self-host + compose stack (MVP)
TAIGA_PUBLIC_PORT=9000
TAIGA_SITES_DOMAIN=localhost:9000
TAIGA_SECRET_KEY=change_me
TAIGA_DB_NAME=taiga
TAIGA_DB_USER=taiga
TAIGA_DB_PASSWORD=change_me
TAIGA_RABBIT_USER=taiga
TAIGA_RABBIT_PASSWORD=change_me

# Planka self-host + compose stack (MVP)
# For IP-only deployments set PLANKA_BASE_URL=http://<SERVER_IP>:3000
PLANKA_PUBLIC_BIND=0.0.0.0
PLANKA_PUBLIC_PORT=3000
PLANKA_BASE_URL=http://localhost:3000
PLANKA_SECRET_KEY=change_me
PLANKA_DB_NAME=planka
PLANKA_DB_USER=planka
PLANKA_DB_PASSWORD=change_me
PLANKA_DEFAULT_ADMIN_EMAIL=
PLANKA_DEFAULT_ADMIN_USERNAME=
PLANKA_DEFAULT_ADMIN_NAME=
PLANKA_DEFAULT_ADMIN_PASSWORD=

# CyberneticAgents container runtime (compose)
CYBERAGENT_DB_URL=sqlite:///data/CyberneticAgents.db
CYBERAGENT_ACTIVE_TEAM_ID=1

# Taiga adapter/worker contract (optional; used by Taiga bridge flows)
TAIGA_BASE_URL=
TAIGA_TOKEN=
TAIGA_PROJECT_SLUG=cyberneticagents
TAIGA_ASSIGNEE=taiga-bot
TAIGA_SOURCE_STATUS=pending
TAIGA_TARGET_STATUS=completed
TAIGA_RESULT_COMMENT=Automated result: completed by CyberneticAgents Taiga PoC adapter.
