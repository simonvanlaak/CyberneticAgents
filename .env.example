# Copy to .env and fill real values (do not commit .env)

# Required for Groq/Mistral
GROQ_API_KEY=
MISTRAL_API_KEY=

# Langfuse (optional)
LANGFUSE_SECRET_KEY=
LANGFUSE_PUBLIC_KEY=
LANGFUSE_BASE_URL=https://cloud.langfuse.com

# LangSmith (optional)
# LANGSMITH_API_KEY=
# LANGSMITH_TRACING=true
# LANGSMITH_ENDPOINT=https://eu.api.smith.langchain.com
# LANGSMITH_PROJECT=CyberneticAgents

# LLM config (optional)
LLM_PROVIDER=groq
GROQ_MODEL=llama-3.3-70b-versatile
MISTRAL_MODEL=mistral-small-latest
LLM_MAX_TOKENS=4096
GROQ_BASE_URL=https://api.groq.com/openai/v1

# Telegram (optional)
TELEGRAM_BOT_TOKEN=
TELEGRAM_WEBHOOK_URL=
TELEGRAM_WEBHOOK_SECRET=
TELEGRAM_WEBHOOK_HOST=0.0.0.0
TELEGRAM_WEBHOOK_PORT=8080
TELEGRAM_ALLOWLIST_CHAT_IDS=
TELEGRAM_ALLOWLIST_USER_IDS=
TELEGRAM_BLOCKLIST_CHAT_IDS=
TELEGRAM_BLOCKLIST_USER_IDS=
